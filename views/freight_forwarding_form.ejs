<%- include('./partials/header'); -%>

    <title>Freight Forwarding Form</title>

    <style>
        body {
            font-family: 'Open Sans', sans-serif;
        }

        h1 {
            margin: 1rem auto;
            font-family: 'Antic Didone', serif !important;
        }

        h4 {
            font-family: 'Merriweather', serif !important;
            color: #536DFE;
        }

        .form {
            width: 70rem;
            margin: 0 auto;
            padding: 3% 7%;
        }

        .heading {
            text-align: center;
        }

        .form form {
            text-align: left;
        }

        .row {
            margin-top: 1.5rem;
            margin-bottom: 1.5rem;
        }

        .form-group {
            margin: 0.7rem auto;
        }

        label {
            display: block;
        }

        select {
            padding: 0.5rem;
            border-radius: 5px;
        }

        select option {
            font-size: 17px;
        }

        form input {
            width: 75%;
            padding: 0.5rem;
            border: 1px solid rgba(0, 0, 0, 0.5);
            border-radius: 5px;
        }

        form .user-group,
        form .shipment-group {
            display: flex;
        }

        form .user-group .form-group {
            margin-left: 0%;
            margin-right: 0%;
        }

        form .shipment-group .form-group {
            margin-left: 3%;
            margin-right: 3%;
        }

        form .user-group {
            position: relative;
            left: 0.1rem;
        }

        form .shipment-group {
            position: relative;
            left: -2.8rem;
        }

        form #user-phone-code,
        #user-phone {
            width: 90%;
        }

        form #pickup-phone-code,
        #pickup-phone {
            width: 110%;
        }

        button {
            cursor: pointer;
            border: 2px solid #6CFE52;
            border-radius: 10px;
            padding: 0.7rem 1.5rem;
            background-color: #6CFE52;
            color: #fff;
            transition: all 0.3s;
        }

        button i {
            margin-top: 0.3rem;
            margin-left: 0.2rem;
            transition: all 0.3s;
        }

        button:hover {
            box-shadow: 5px 10px 10px 5px #6cfe525e;
            color: #fff;
            background-color: #6cfe52;
            transform: scale(1.07);
        }

        button:hover i {
            margin-left: 0.5rem;
        }

        footer {
            display: flex;
            justify-content: space-between;
            background-color: #121212e3;
            color: #fff;
            margin-top: 1rem;
        }

        footer p {
            margin: 1.5rem;
        }

        footer .dev-name a {
            color: #FE526C;
            text-decoration: underline;
        }

        @media (max-width: 1200px) {
            form input {
                width: auto;
            }

            form select {
                width: auto;
            }
        }

        @media (max-width: 769px) {

            body {
                overflow-x: hidden;
            }

            .heading {
                text-align: left;
            }


            #user-phone-code,
            #user-phone,
            #pickup-phone-code,
            #pickup-phone {
                width: auto !important;
            }

            footer {
                display: block;
                text-align: center;
                padding: 1rem;
            }

            footer p {
                margin: 0.3rem;
            }
        }
    </style>

